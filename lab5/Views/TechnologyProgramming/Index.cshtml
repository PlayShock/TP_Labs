        @Html.LabelFor(m => m.TechnologyName): @Html.DisplayFor(m => m.TechnologyName)
    </div>
    <div>
        @Html.LabelFor(m => m.Language): @Html.DisplayFor(m => m.Language)
    </div>
    <div>
        @Html.LabelFor(m => m.Version): @Html.DisplayFor(m => m.Version)
    </div>
