        @Html.LabelFor(m => m.ReleaseDate): @Html.DisplayFor(m => m.ReleaseDate)
    </div>
    <div>
        @Html.LabelFor(m => m.Developer): @Html.DisplayFor(m => m.Developer)
    </div>
    <div>
